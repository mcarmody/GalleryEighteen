<%= render 'layouts/breadcrumbs' %>

<div class = "container">

  <!-- <h1>Listing items</h1> -->
      <div id="masonry-container" class="store-container transitions-enabled infinite-scroll">
        <% @items.each do |item| %>
          <div class="item box small-2 large-4 columns">
            <%= image_tag item.pic %>
            <div class="art-label">
              <%= link_to item.title, item %>
              <div class="artist">by <%= item.artist %></div>
            </div>
          </div>
        <% end %>
      </div>
  </div>